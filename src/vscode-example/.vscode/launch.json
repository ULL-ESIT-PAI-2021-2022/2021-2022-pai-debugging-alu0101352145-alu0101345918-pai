{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Collatz",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/collatz-example/the-sequence of-collatz-P80660.js",
      "stopOnEntry": true,
      "args": ["3", "1", "40"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Classification of characters",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/classification-of-characters-example/classification-of-characters.js",
      "stopOnEntry": true,
      "args": ["A"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Reverse of sequences",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/reverse-example/reverse-of-sequences.js",
      "stopOnEntry": true,
      "args": ["3", "2", "3", "4"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Sum of digits",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/sum-of-digits-example/sum-of-digits.js",
      "stopOnEntry": true,
      "args": ["1231", "3010", "000", "7717"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Sum of divisors",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/sum-of-divisors-example/sum-of-divisors.js",
      "stopOnEntry": true,
      "args": ["20"]
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Factorial of the sum of the areas",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/multiple-files-example/factorial-of-sum-of-areas.js",
      "stopOnEntry": true,
      "args": ["0.98", "1.34"]
    }
  ]
}